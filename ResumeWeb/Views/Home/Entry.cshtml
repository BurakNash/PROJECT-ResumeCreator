@using ResumeWeb.Models;
@model List<Entry>
<section id="home" class="tt-fullHeight" data-stellar-vertical-offset="50" data-stellar-background-ratio="0.2">
    @foreach (var item in Model)
    {   
        <div class="intro">
            <div class="intro-sub">@item.Name</div>
            <h1>@item.JobFirstName <span>@item.JobLastName</span></h1>
            <p>
               @item.Introduction
            </p>
            <div class="social-icons">
                <ul class="list-inline">
                    <li><a href="@item.Facebook" target="_blank"><i class="fa fa-facebook"></i></a></li>
                    <li><a href="@item.Linkedin" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                    <li><a href="@item.Github" target="_blank"><i class="fa fa-github"></i></a></li>
                </ul>
            </div> <!-- /.social-icons -->
        </div>
    }

    <div class="mouse-icon">
        <div class="wheel"></div>
    </div>
</section>